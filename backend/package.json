{
  "name": "proshop",
  "version": "1.0.0",
  "description": "eCommerce application built withthe MERN stack",
  "main": "backend/dist/server.js",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "construct": "tsc",
    "start": "node dist/server.js",
    "client": "npm run dev --prefix ../frontend",
    "proj": "concurrently \"npm run dev --prefix backend\" \"npm run client\"",
    "data:import": "ts-node src/seeder.ts",
    "data:destroy": "ts-node src/seeder.ts -d",
    "install:all": "npm install && cd frontend && npm install",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "npm run build --prefix ../frontend" 
},
  "author": "Emanuelly Leoncio",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.2",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.12.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "concurrently": "^9.1.0",
    "dotenv": "^16.4.7",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
